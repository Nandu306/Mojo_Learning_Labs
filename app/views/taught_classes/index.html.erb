<% @taught_classes.each do |taught_class| %>

  <div class="d-flex">
    <%= taught_class.year %>
    <%= taught_class.subject %>
    <%= taught_class.academic_year %>

    <% if current_user.student? %>

      <%= link_to 'Join class', taught_class_class_memberships_path(taught_class), method: :post, remote: true %>

    <% end %>
  </div>


<% end %>


