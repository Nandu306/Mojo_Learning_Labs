<%= simple_form_for ([@taught_class, @assignment]) do |f| %>
<%= f.input :topic %>
<%= f.input :deadline %>


<%= f.abyme_for(:questions, partial: 'assignments/question_fields') do |abyme| %>
    <%= abyme.records %>
    <%= abyme.new_records %>
    <%= add_associated_record %>
<% end %>



<%= f.button :submit, "Save", class: "btn btn-primary" %>


<% end %>
